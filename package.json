  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "iconv-lite": "^0.5.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.20",
